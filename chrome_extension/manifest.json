{
  "name": "Chrotools",
  "version": "0.9.0",
  "icons": { "128": "timg.png" },
  "manifest_version": 2,
  "description": "浏览器小工具",
  "browser_action": {
    "default_icon": "timg.png" ,
    "default_title": "Chrotools",
    "default_popup": "index.html"
  },
  "content_scripts":[
    {
      "matches":["https://leetcode.com/problems/*"],
      "js":["js/jquery.2.1.4.js","js/db.js"],
      "runat":"document_end"
    }
  ],
  "permissions":["tabs","https://www.baidu.com/"]
}